{
  "name": "@microsoft/ai-chat-protocol-model",
  "version": "0.1.0-alpha.1",
  "description": "TypeScript models for the AI Chat Protocol requests and responses.",
  "main": "./dist/commonjs/index.js",
  "scripts": {
    "build": "tshy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/ai-chat-protocol.git"
  },
  "author": "Gerardo Lecaros <gerardo.lecaros.e@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/ai-chat-protocol/issues"
  },
  "homepage": "https://github.com/microsoft/ai-chat-protocol#readme",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tshy": "^1.12.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@typespec/ts-http-runtime": "^1.0.0-alpha.20240228.1"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    },
    "dialects": [
      "esm",
      "commonjs"
    ],
    "esmDialects": [
      "browser"
    ],
    "selfLink": false
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "types": "./dist/commonjs/index.d.ts",
  "type": "module"
}
